
package: de.hshannover.f4.trust.irongenlog.publisher.strategies 

class: DhcpStrategy

	publishLogData:

		update:
			identifier1 = ar("MAC")			
			identifier2 = dev("DHCPSERVERNAME")
			meta = unexpectedBehavior("a","b","c","d",critical,"f")
			lifetime = session
		
		delete:
			identifier1 = mac("MAC")
			identifier2 = dev("DHCPSERVERNAME")
			deleteString = 'meta:ip-mac[@ifmap-publisher-id='" + ssrc.getPublisherId() + "']'
												
		notify:
			identifier1 = mac("MAC")
			identifier2 = dev("DHCPSERVERNAME")
			meta = discoBy()

	method: blub

		delete:
			identifier1 = identityEx('rootNode.path( "MAC" ).getTextValue()')
			deleteString = 'meta:ip-mac[@ifmap-publisher-id='" + ssrc.getPublisherId() + "']'


